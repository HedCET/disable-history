{
  "background": {
    "persistent": true,
    "scripts": ["background.scripts.js"]
  },
  "browser_action": {
    "default_icon": "enabled.png",
    "default_title": "History Enabled"
  },
  "description": "Selectively/Temporary Enable/Disable Browser History",
  "homepage_url": "https://github.com/HedCET/disable-history/wiki",
  "icons": {
    "48": "enabled.png"
  },
  "manifest_version": 2,
  "name": "History On/Off Switch",
  "offline_enabled": true,
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": ["downloads", "history", "storage", "tabs"],
  "version": "1.3"
}
