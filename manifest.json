{
  "background": {
    "persistent": true,
    "scripts": [
      "background.scripts.js"
    ]
  },
  "browser_action": {
    "default_icon": "enabled.png",
    "default_title": "History Enabled"
  },
  "description": "History On/Off Switch",
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://disable-history.web.app/*"
    ]
  },
  "icons": {
    "48": "enabled.png"
  },
  "manifest_version": 2,
  "name": "History On/Off",
  "permissions": [
    "tabs",
    "history"
  ],
  "version": "1.1"
}